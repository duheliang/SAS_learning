data adlb1;
  set adlb;
    where paramcd='ANA' and saffl='Y';
run;

/****************************** 生成base和avalc数据集，方便分别计数total ******************************/
data adlb2(keep=usubjid avalc rename=(avalc=BASE))
		adlb3(keep=trtan usubjid avalc);
  set adlb1;
    if ablfl='Y' then output adlb2;
	else if avisitn=5 then output adlb3;
run;

data adlb4;
  merge adlb2 adlb3;
    by usubjid;
    if base='' then base='Missing';
	if avalc='' then avalc='Missing';
run;

/*************************************** 计数的主体部分freq1 ****************************************/
proc freq data=adlb4;
  tables trtan*base*avalc/out=freq1;
run;

proc freq data=adlb4;               *列的总计;
  tables  trtan*avalc/out=total_avalc;
run;

proc freq data=adlb4;               *行的总计;
  tables trtan*base/out=total_base;
run;

proc freq data=adlb4;               *总计;
  tables trtan/out=total_total;
run;

data freq2;                                                     *填补，多试试;
  set freq1 total_avalc(in=a) total_base(in=b) total_total(in=c);
    if a then base='Total';
	if b then avalc='Total';
	if c then do;
	  avalc='Total';
	  base='Total';
	end;
  drop percent;
run;

proc sort data=freq2 out=freq3;
  by trtan base avalc;
run;

/************************************** 计算总人数tot作为分母 ***************************************/
proc sort data=adlb1 out=adlb5 nodupkey;
  by usubjid;
run;

proc freq data=adlb5;
  tables trtan/out=tot(rename=(count=tot));
run;

data combine;
  merge freq3 tot(drop=percent);
    by trtan;
  length col $20;
    col=put(count,3.)||' ('||put(round(count/tot*100,0.1),5.1)||')';
run;

proc sort data=combine out=combine1;
  by trtan base avalc;
run;

proc transpose data=combine1 out=combine2(drop=_name_);
  by trtan base;
  id avalc;
  var col;
run;

/*********************************** 将呈现的数据结构同shell一致 ************************************/
data dummy;
  trtan=1;base='Positive';output;
  trtan=1;base='Missing';output;
run;

proc sort data=dummy;by trtan base;run;

data combine3;
  merge combine2 dummy;
    by trtan base;
    array p[4] positive total negative missing; *利用数组用0填补缺失值;
      do i=1 to 4;
	    if missing(p[i]) then p[i]='  0';
	  end;
	if base='Negative' then id=1;
	  else if base='Positive' then id=2;
	  else if base='Missing' then id=3;
	  else if base='Total' then id=4;
    if trtan=1 then trta='Drug A';
	  else if trtan=2 then trta='Drug B';
	drop i;
run;

proc sort data=combine3 out=combine4;
  by trtan id;
run;

data final(drop=trtan id);                                 *retain于set前确定变量位置;
  retain trta base negative positive positive missing total;
  set combine4;
run;
