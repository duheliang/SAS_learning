/********************** 挑选出符合要求的观测，产生数据集adlb4，包含相应的chg变量 **********************/
proc sql;
  create table adlb1 as
  select *
    from adlb
	where saffl='Y' and paramcd='WBC'
	order by 3,6;

  create table adlb2 as
  select usubjid,aval as BASE
    from adlb
    where saffl='Y' and paramcd='WBC' and ablfl='Y'
	order by 1;
quit;

data adlb3;
  merge adlb1 adlb2;
    by usubjid;
run;

data adlb4;
  set adlb3;
    by usubjid avisitn;
    if  avisitn>=3 then
	  CHG=aval-base;
run;

/*********** 对第一周之后的aval进行统计，标记为cat=1；对第二周之后的chg进行统计，标记为cat=2 ***********/
proc means data=adlb4(where=(avisitn>=2)) nway;
  class trtan avisitn;
  var aval;
    output out=test1(drop=_type_ _freq_)
	  n=_n mean=_mean std=_std median=_median q1=_q1 q3=_q3 min=_min max=_max;
run;

proc means data=adlb4(where=(avisitn>=3)) nway;
  class trtan avisitn;
  var chg;
    output out=test2(drop=_type_ _freq_)
	  n=_n mean=_mean std=_std median=_median q1=_q1 q3=_q3 min=_min max=_max;
run;

data test;
  set test1(in=a) test2(in=b);
    if a then cat=1;
	if b then cat=2;
  length N1-N5 $100;
    N1=put(round(_n,1),5.);      *****************默认用put函数进行四舍五入可能会出错*****************;
	N2=put(round(_mean,0.1),7.1)||' ('||put(round(_std,0.2),8.2)||')';
	N3=put(round(_median,0.1),7.1);
	N4=put(round(_q1,0.1),7.1)||'-'||put(round(_q3,0.1),7.1);
	N5=put(round(_min,1),5.)||','||put(round(_max,1),5.);
  drop _:;
run;

/***************************************** 转置为需要的布局 ******************************************/
proc sort data=test out=test_t;
  by avisitn cat trtan;
run;

proc transpose data=test_t out=tot prefix=col;
  by avisitn cat;
  id trtan;
  var N1-N5;
run;

data tot1;
  length item $20;
  set tot;
    if _name_='N1' then item='  N';
	  else if _name_='N2' then item='  Mean (SD)';
      else if _name_='N3' then item='  Median';
	  else if _name_='N4' then item='  IQR (Q1-Q3)';
	  else if _name_='N5' then item='  Range';
run;

/****************************************** 进行Title说明 *******************************************/
data dummy;
  length item $20;
    item='Week 1';avisitn=2;cat=1;output;
